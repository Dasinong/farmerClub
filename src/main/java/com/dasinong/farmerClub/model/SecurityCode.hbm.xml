<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.farmerClub.model.SecurityCode" table="securitycode" catalog="farmer_club">
        <id name="codeId" type="long">
            <column name="codeId" />
            <generator class="identity" />
        </id>
        <property name="code" type="string">
            <column name="code" length="30" not-null="true" />
        </property>
    <property name="createdAt" type="timestamp" column="createdAt" />
    <property name="expiredAt" type="timestamp" column="expiredAt" />
    </class>
</hibernate-mapping>