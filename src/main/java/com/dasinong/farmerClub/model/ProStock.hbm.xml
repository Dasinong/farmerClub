<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.farmerClub.model.ProStock"
        table="prostock" catalog="farmer_club">
        <id name="id" type="long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="userid" type="long" column="userid"/>
        <property name="prodid" type="long" column="prodid"/>
        <property name="prodname" type="string" column="prodname"/>
        <property name="prospecial" type="string" column="prospecial"/>
        <property name="scanat" type="timestamp" column="scanat" />
        <property name="boxcode" type="string" column="boxcode"/>
    </class>
</hibernate-mapping>