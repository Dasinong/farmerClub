<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.farmerClub.model.Task" table="task" catalog="farmer_club">
        <id name="taskId" type="java.lang.Long">
            <column name="taskId" />
            <generator class="identity" />
        </id>
        <property name="taskSpecId" type="java.lang.Long">
            <column name="taskSpecId" />
        </property>
        <property name="fieldId" type="java.lang.Long">
            <column name="fieldId" />
        </property>
        <property name="taskStatus" type="java.lang.Boolean">
            <column name="taskStatus" />
        </property>
    </class>
</hibernate-mapping>