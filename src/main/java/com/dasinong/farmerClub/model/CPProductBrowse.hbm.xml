<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dasinong.farmerClub.model.CPProductBrowse" table="cpproductBrowse" catalog="farmer_club">
        <id name="cPProductId" type="java.lang.Long">
            <column name="cPProductId" />
            <generator class="identity" />
        </id>
        <property name="model" type="string">
        	<column name="model" length="20"/>
        </property>
        <property name="activeIngredient" type="string">
            <column name="activeIngredient" length="50"/>
        </property>
        <property name="activeIngredientPY" type="string">
        	<column name="activeIngredientPY" length="50"/>
        </property>
    </class>

</hibernate-mapping>